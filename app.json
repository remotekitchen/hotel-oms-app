{
  "expo": {
    "name": "hotel-oms-app",
    "slug": "hotel-oms-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appIcon/app_icon.png",
    "scheme": "com.chatchefs.hoteloms",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app requires access to the camera.",
        "NSPhotoLibraryUsageDescription": "This app requires access to the photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appIcon/app_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET",
        "NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.chatchefs.hoteloms"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/appIcon/app_icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/appIcon/app_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "441bd409-d5fb-4db1-bda0-15bf4e0f08ae"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/441bd409-d5fb-4db1-bda0-15bf4e0f08ae"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
